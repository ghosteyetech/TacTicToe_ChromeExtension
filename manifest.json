{
  "manifest_version": 2,

  "name": "TacTicTuk background Process",
  "description": "TacTicTuk background Process testing extention",
  "version": "1.0",

  "icons": {
    "128": "img/img.png"
  },

  "browser_action": {
    "default_icon": "img/img.png",
    "default_popup": "home.html",
    "default_title": "TacTicTuk background Process"
  },

  "background" :{
    "scripts" : ["js/background.js"],
    "persistent" : true
  },

  "web_accessible_resources": [
    "img/img.png"
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "permissions": [ "http://*/*", "https://*/*", "unlimitedStorage", "contextMenus", "cookies", "tabs", "notifications" ]
}
